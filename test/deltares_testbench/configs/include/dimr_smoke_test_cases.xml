    <programs>
      <program ref="execute_container" seq="1">
        <arguments>
          <argument>run_dflowfm.sh</argument>
          <argument>--partition:ndomains=4:icgsolver=6</argument>
          <argument>t01.mdu</argument>
        </arguments>
